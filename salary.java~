import java.util.Scanner;
public class salary
{
  public static void main(String [] args)
  {
    Scanner key=new Scanner(System.in);
    int t=key.nextInt();
    int min,max,n,i;
    if(t<20)
    {
    int [] man=new int[3];
    i=0;
    while(i<man<length)
    {
    man[i]=key.nextInt();
    n=n+man[i];
    i++;
    }
    i=0;
      max=man[i];
      min=man[i];
      while(i<man.length)
      {
        if(man[i]>max)
        {
          max=man[i];
        }
        else if(man[i]<min)
        {
          min=man[i];
        }
        i++;
      }
      n=n-(max=min);
      System.out.println(n);
    t--;
    }