import java.util.Scanner;
public class u12141
{
  public static void main(String [] args)
  {
    Scanner key=new Scanner(System.in);
    int t=key.nextInt();
    while(key.hasNext())
    {
      int sum=0;
    for(int i=1;i<=t;i++)
    {
    sum=sum+(i*i);
    }
    System.out.println(sum);
    }
  }
}