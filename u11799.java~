import java.util.Scanner;
public class u11799
{
  public static void main(String [] args)
  {
    int t=key.nextInt();
    while(t>0)
    {
      int k=key.nextInt();
      int [] man=new int[k];
      int max=man[0];
      for(int i=0;i<k;i++)
      {
        man[i]=key.nextInt();
      }
      for(i=0;i<k;i++)
      {
        if(max<man[i])
        {
          max=man[i];
        }
      }
      t--;
    }
  }
}